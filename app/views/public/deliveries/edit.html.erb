<div class="col-md-12">
  <h3><span style="background-color: gray;">配送先編集</span></h3>
  <%= form_with model: @delivery, local: true do |f| %>
    <div class="form-group">
      <%= f.label :postcode %>
      <%= f.text_field :postcode%>
    </div>

    <div class="form-group">
      <%= f.label :address %>
      <%= f.text_field :address%>
    </div>

    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>

    <%= f.submit "編集する", class: "btn btn-primary text-right" %>
    
  <% end %>
</div>